#include <stdio.h>

int main()
{
        int grade=92;
        if(grade>=100&& grade>=98)
        {
            printf("1.0");
        }
        else if(grade>=97&&grade>=94)
        {
            printf("1.25");
        }
        else if(grade>=93&&grade>=89)
        {
            printf("1.5");
        }
        else{
            printf("2.0");
        }
}
